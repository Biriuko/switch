#include <iostream>
using namespace std;

void main()
{

	int month = 0;
	cout << "Enter number of month: ";
	cin >> month;

	switch (month)
	{
	case 12:
	case 1:
	case 2:
		cout << "Winter " << endl; break;
	case 3:
	case 4:
	case 5:
		cout << "Spring " << endl; break;
	case 6:
	case 7:
	case 8:
		cout << "Summer " << endl; break;
	case 9:
	case 10:
	case 11:
		cout << "Auterm" << endl; break;

	default: cout << "Vu mozhete vvodit tolko -0 && >12\n";
	}

		system ("pause");
	}
